{
  "name": "datocms-plugin-bridged-links",
  "homepage": "https://github.com/SYMBIO/datocms-plugin-bridged-links",
  "repository": {
    "type": "git",
    "url": "https://github.com/SYMBIO/datocms-plugin-bridged-links.git"
  },
  "version": "0.0.2",
  "description": "Bridging field values from selected path to multilink fields",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --port 5000",
    "dist": "NODE_ENV=production webpack --progress",
    "build": "NODE_ENV=production webpack --progress",
    "addToProject": "yo datocms-plugin:add-to-project",
    "prepublishOnly": "rimraf lib public && mkdir public && npm run dist",
    "lint": "eslint \"./src/*.{js,jsx}\"",
    "test": "echo 1"
  },
  "keywords": [
    "datocms",
    "datocms-plugin",
    "field-editor",
    "multiple-links"
  ],
  "files": [
    "public",
    "docs"
  ],
  "author": "SYMBIO <dev@symbio.agency>",
  "license": "ISC",
  "datoCmsPlugin": {
    "title": "Bridged Links",
    "previewImage": "docs/preview.gif",
    "coverImage": "docs/cover.png",
    "entryPoint": "dist/index.html",
    "pluginType": "field_editor",
    "fieldTypes": [
      "links"
    ],
    "parameters": {
      "global": [
        {
          "id": "datoCmsApiToken",
          "label": "DatoCMS API Token",
          "type": "string",
          "required": true,
          "hint": "The DatoCMS API read-only token, <a href=\"/admin/access_tokens\">get it here</a>"
        }
      ],
      "instance": [
        {
          "id": "apiKey",
          "label": "Path",
          "type": "string",
          "required": true,
          "hint": "Path to field to display (e.g. advertise.title)"
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.14",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "generator-datocms-plugin": "^1.0.27",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^4.5.2",
    "html-webpack-tags-plugin": "^3.0.1",
    "mini-css-extract-plugin": "^1.4.1",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "yo": "^3.1.1"
  },
  "dependencies": {
    "classname": "^0.0.0",
    "interactjs": "^1.10.11",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  }
}
